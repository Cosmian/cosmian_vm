[program:cosmian_vm_agent]
command=cosmian_vm_agent $COSMIAN_VM_AGENT_CERTIFICATE
directory=/usr/sbin
autostart=true
autorestart=true
user=root
stderr_logfile=/var/log/cosmian_vm/agent.err.log
stdout_logfile=/var/log/cosmian_vm/agent.out.log
environment=COSMIAN_VM_AGENT_CERTIFICATE="COSMIAN_VM_DN_PLACEHOLDER"